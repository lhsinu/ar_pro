<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingDeviceActivity">

    <TextView
        android:id="@+id/tv_gas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:text="@string/strGas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_gas_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:text="@android:string/ok"
        app:layout_constraintStart_toEndOf="@+id/tv_gas"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_gas_1"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_gas" />

    <EditText
        android:id="@+id/et_gas_2"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gas_1"
        app:layout_constraintTop_toBottomOf="@+id/tv_gas" />

    <EditText
        android:id="@+id/et_gas_3"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gas_2"
        app:layout_constraintTop_toBottomOf="@+id/tv_gas" />

    <EditText
        android:id="@+id/et_gas_4"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gas_3"
        app:layout_constraintTop_toBottomOf="@+id/tv_gas" />

    <EditText
        android:id="@+id/et_gas_5"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gas_4"
        app:layout_constraintTop_toBottomOf="@+id/tv_gas" />

    <EditText
        android:id="@+id/et_gas_6"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gas_5"
        app:layout_constraintTop_toBottomOf="@+id/tv_gas" />

    <ImageView
        android:id="@+id/iv_line1"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/et_gas_1" />

    <TextView
        android:id="@+id/tv_gyro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:text="@string/strGyro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_line1" />

    <Button
        android:id="@+id/bt_gyro_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:text="@android:string/ok"
        app:layout_constraintStart_toEndOf="@+id/tv_gyro"
        app:layout_constraintTop_toBottomOf="@id/iv_line1" />

    <EditText
        android:id="@+id/et_gyro_1"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_gyro" />

    <EditText
        android:id="@+id/et_gyro_2"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gyro_1"
        app:layout_constraintTop_toBottomOf="@+id/tv_gyro" />

    <EditText
        android:id="@+id/et_gyro_3"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gyro_2"
        app:layout_constraintTop_toBottomOf="@+id/tv_gyro" />

    <EditText
        android:id="@+id/et_gyro_4"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gyro_3"
        app:layout_constraintTop_toBottomOf="@+id/tv_gyro" />

    <EditText
        android:id="@+id/et_gyro_5"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gyro_4"
        app:layout_constraintTop_toBottomOf="@+id/tv_gyro" />

    <EditText
        android:id="@+id/et_gyro_6"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="00"
        android:maxLength="2"
        app:layout_constraintStart_toEndOf="@+id/et_gyro_5"
        app:layout_constraintTop_toBottomOf="@+id/tv_gyro" />



    <ImageView
        android:id="@+id/iv_line2"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/et_gyro_1" />


    <Button
        android:id="@+id/bt_Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="50dp"
        android:text="@string/strReset"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/et_gyro_1" />

</androidx.constraintlayout.widget.ConstraintLayout>